#include<iostream>
#include<cmath>
using namespace std;

int main() {
    double a, b, c;
    cout << "Ecuacion: ax^2 + bx + c = 0" << endl;
    cout << "a = "; cin >> a;
    cout << "b = "; cin >> b;
    cout << "c = "; cin >> c;
    
    if (a == 0) {
        cout << "No es cuadratica (a no puede ser 0)" << endl;
        return 0;
    }
    
    double discriminante = b*b - 4*a*c;
    
    if (discriminante > 0) {
        double x1 = (-b + sqrt(discriminante)) / (2*a);
        double x2 = (-b - sqrt(discriminante)) / (2*a);
        cout << "Dos soluciones reales: x1 = " << x1 << ", x2 = " << x2 << endl;
    } else if (discriminante == 0) {
        double x = -b / (2*a);
        cout << "SoluciÃ³n doble: x = " << x << endl;
    } else {
        cout << "No hay soluciones reales (discriminante negativo)" << endl;
    }
    
    return 0;
}
