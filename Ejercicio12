#include <iostream>
using namespace std;

int main() {
    int dia, mes, anio;
    cout << "Ingrese fecha (dd mm aaaa): ";
    cin >> dia >> mes >> anio;
    
    string estacion;
    
    if ((mes == 9 && dia >= 21) || mes == 10 || mes == 11 || (mes == 12 && dia < 21)) {
        estacion = "PRIMAVERA";
    } else if ((mes == 12 && dia >= 21) || mes == 1 || mes == 2 || (mes == 3 && dia < 21)) {
        estacion = "VERANO";
    } else if ((mes == 3 && dia >= 21) || mes == 4 || mes == 5 || (mes == 6 && dia < 21)) {
        estacion = "OTOÃ‘O";
    } else {
        estacion = "INVIERNO";
    }
    
    cout << "Estacion: " << estacion << endl;
    
    return 0;
}
