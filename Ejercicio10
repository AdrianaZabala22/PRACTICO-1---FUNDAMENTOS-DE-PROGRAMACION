#include <iostream>
using namespace std;

int main() {
    int h1, m1, s1, h2, m2, s2;
    
    cout << "Hora inicial (HH MM SS): ";
    cin >> h1 >> m1 >> s1;
    cout << "Hora final (HH MM SS): ";
    cin >> h2 >> m2 >> s2;
    
    // Convertir todo a segundos
    int seg1 = h1*3600 + m1*60 + s1;
    int seg2 = h2*3600 + m2*60 + s2;
    
    int diferencia = seg2 - seg1;
    
    int dh = diferencia / 3600;
    int dm = (diferencia % 3600) / 60;
    int ds = diferencia % 60;
    
    cout << "Diferencia: " << dh << " Hr " << dm << " Min " << ds << " Seg" << endl;
    
    return 0;
}
